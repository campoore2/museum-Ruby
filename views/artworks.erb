<h2>Current Museums</h2>
<h6>Click on a museum to add art.</h6>
<% if @museums.any?() %>
  <% @museums.each() do |museum| %>
  <h5><a class="btn btn-default" href="/curator/artworks/add/<% museum.name() %>"><%= museum.name() %></a></h5>
  <% end %>
<% else %>
  <h5>No Museums</h5>
<% end %>

<a class="btn btn-default" href="/">Home</a>
<a class="btn btn-default" href="/curator/museums">Back</a>
